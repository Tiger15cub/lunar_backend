{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "skipDefaultLibCheck": true,
    "types": ["node", "fastify", "ws"],
    "outDir": "./dist"
  },
  "include": ["src/*", "model/*", "resources/*"],
  "exclude": ["node_modules"],
  "typeRoots": ["node_modules/@types"]
}
